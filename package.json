    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.0",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "marked": "1.0.0",
    "protractor": "6.0.0",
    "rimraf": "3.0.2",
    "ts-node": "8.10.1",
    "tslint": "5.20.1",
    "typescript": "3.7.5",
    "uuid": "8.0.0",
    "yargs": "15.3.1"
  }
